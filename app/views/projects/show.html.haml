- content_for :toolbar do
  #assign-project-collaborators
    .pull-left
      %span.label.info-text
        Drag & drop, assign collaborators
        %i.icon-long-arrow-right
    - @project.users.each do |user|
      = image_tag avatar_url(user), class: 'avatar-image avatar-image-md img-circle draggable', id: user.id , title: user.name

.vertical-scroll{style: "width: #{@lists.count*315}px"}
  =render @lists
