- title 'Projects'
- content_for :toolbar do
  = link_to new_project_path, class: 'btn btn-default' do
    %i.icon-plus
    = t('action_button.new', resource_name: Project.model_name.human)

.panel.panel-default.grid
  =blankable(@projects)
  .panel-footer
    .pagination.pagination-sm
      = will_paginate @projects, renderer: BootstrapPagination::Rails, bootstrap: 3